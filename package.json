{
    "name": "raft-kv-store",
    "version": "1.0.0",
    "description": "Distributed Key-Value Store with Raft Consensus Protocol",
    "main": "dist/server.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/server.js",
        "start:client": "node dist/client/cli.js",
        "dev": "ts-node src/server.ts",
        "dev:client": "ts-node src/client/cli.ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage"
    },
    "keywords": [
        "raft",
        "consensus",
        "distributed-systems",
        "key-value-store"
    ],
    "license": "MIT",
    "dependencies": {
        "express": "^4.18.2",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.11",
        "@types/node": "^20.10.0",
        "@types/uuid": "^9.0.7",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.0"
    }
}